# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log
/tmp
/public/uploads
.ruby-version
public/assets/
vendor/bundle/
.DS_Store
# Ignore project files
cookinme.sublime-project
cookinme.sublime-workspace
/app/assets/img_examples/
# Ignore application configuration
/config/application.yml
